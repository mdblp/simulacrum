  "dependencies": {
    "@effection/atom": "2.0.0-preview.13",
    "@effection/node": "2.0.0-preview.14",
    "@envelop/core": "^0.3.1",
    "@simulacrum/server": "0.2.0",
    "effection": "2.0.0-preview.14",
