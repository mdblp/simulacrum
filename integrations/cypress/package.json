{
  "name": "@simulacrum/cypress",
  "version": "0.0.1",
  "description": "Cypress simulacrum commands",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "repository": "https://github.com/thefrontside/simulacrum.git",
  "author": "Frontside Engineering <engineering@frontside.com>",
  "license": "MIT",
  "files": [
    "dist/**/*",
    "src/**/*",
    "README.md"
  ],
  "scripts": {
    "cypress:open": "cypress open",
    "start:server": "cd ../../examples/nextjs-with-auth0-react && npm install && npm run dev",
    "cypress:run": "npx cypress run --headless 'cypress/integration/*.ts'",
    "start:auth0": "PORT=4000 npx auth0-simulator",
    "test": "npm-run-all --parallel start:auth0 start:server cypress:run",
    "build": "npm run prepack",
    "prepack": "tsc --build tsconfig.dist.json",
    "lint":  "eslint \"src/**/*.ts\""
  },
  "dependencies": {
    "@simulacrum/auth0-simulator": "^0.2.1",
    "@auth0/auth0-spa-js": "^1.16.1",
    "cypress": "^8.1.0"
  },
  "devDependencies": {
    "@frontside/eslint-config": "^2.0.0",
    "@frontside/tsconfig": "^1.2.0",
    "@frontside/typescript": "^1.1.1",
    "npm-run-all": "^4.1.5"
  },
  "volta": {
    "extends": "../../package.json"
  }
}
